          <script>
             /*--  Country --*/

    const settings = {
        "async": true,
        "crossDomain": true,
        "url": "https://localhost:7155/Admin/getCountry/",
        "method": "POST",
        "headers": {},
        "data": {}
    };

    $.ajax(settings).done(function (response) {
        console.log(response);

        var data = JSON.parse(response);

        var html='';
        html +=`<option value="" disabled selected>--Select Country--</option>`  ;
        for (var i = 0; i < data.length; i++) {
            var element = data[i];
            html += `<option value="${element.CountryId}">${element.CountryName}</option>`;
        }

        $("#CountryId").html(html);
    });

   
        </script>

